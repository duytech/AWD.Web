@using AWD.Web.Core.ViewModels 
@model IList<UserViewModel>
@{ ViewData["Title"] = "User page"; }

    <table class="table table-hover">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
        </tr>
        <tr>
            @{ foreach(var item in Model) {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Age</td>
                </tr>
            } }
        </tr>
    </table>